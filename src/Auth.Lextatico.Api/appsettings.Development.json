{
  "TokenConfiguration": {
    "Audience": "LextaticoApp",
    "Issuer": "LextaticoApp",
    "Seconds": 3600,
    "SecondsRefresh": 7200
  },
  "ConnectionStrings": {
    "LextaticoContext": "Server=sqlserver;Database=Auth.Lextatico;User Id=sa;Encrypt=False;",
    "LextaticoMongoDbLogs": "mongodb://lextatico:L&xtat1c02021@mongodb/lextatico-auth-logs?authSource=admin",
    "RabbitMqAccount": "amqp://lextatico:L&xtat1c02021@rabbitmq3/account"
  },
  "EmailSettings": {
    "Email": "noreply@lextatico.com.br",
    "DisplayName": "Lextatico",
    "Host": "smtp.umbler.com",
    "Port": 587
  },
  "Urls": {
    "LextaticoFront": "http://localhost:3000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithCorrelationId"
    ],
    "Properties": {
      "Application": "Auth.Lextatico.Api"
    },
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341"
        }
      }
    ]
  }
}
